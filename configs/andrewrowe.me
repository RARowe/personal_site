server {
	listen 443 ssl;
  
  ssl_certificate /etc/letsencrypt/live/andrewrowe.me/fullchain.pem;
  ssl_certificate_key /etc/letsencrypt/live/andrewrowe.me/privkey.pem;

  ssl_session_timeout 10m;

	root /var/www/andrewrowe.me;

	index index.html index.htm index.nginx-debian.html;

	server_name andrewrowe.me www.andrewrowe.me;

	location / {
		try_files $uri $uri/ =404;
	}
}